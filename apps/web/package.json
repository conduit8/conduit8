  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --mode development",
    "build": "vite build --mode preview && tsc --noEmit",
    "build:prod": "vite build --mode production && tsc --noEmit",
    "deploy": "pnpm build && wrangler deploy --env preview",
    "deploy:prod": "pnpm build:prod && wrangler deploy --env prod",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run --reporter=dot --silent",
    "test:verbose": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix --quiet",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
