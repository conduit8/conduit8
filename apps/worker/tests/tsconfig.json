{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "@worker/*": ["./src/*"]
    },
    "types": ["../worker-configuration.d.ts", "@cloudflare/vitest-pool-workers"],
    "allowJs": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "../src/**/*"]
}
