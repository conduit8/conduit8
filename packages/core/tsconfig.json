{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ESNext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "types": ["@cloudflare/workers-types"],
    "allowImportingTsExtensions": false

  },
  "include": ["src"]
}
